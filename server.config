  <add key="DOSLIMIT" value="5000"/>

  <add key="EudoLogin" value="eudonet"/>
  <add key="EudoPassword" value="JJmKdsm"/>

  <add key="ServerWithoutInternet" value="0"/>

  <add key="ServerWithoutDBSizeInfos" value="0"/>
