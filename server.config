<?xml version="1.0"?>
<ServerSpecificSettings>
  <!--
  
    * ATTENTION *
    Les modifications dans ce fichier ne sont pas prise "à chaud"
    Il faut un changement même "inutile" dans web.config
    (il faut juste que la date de dernière modification change)
  
    -->
  <!-- Extensions autorisées pour l'upload -->
  <add key="allowedextensions" value="fmuUnt5JZAs="/>

  <!-- port https -->
  <add key="SecureServerPort" value="443"/>

  <!-- url de la v7 du serveur -->
  <add key="v7dir" value="v7"/>

  <!-- url de la XRM du serveur -->
  <add key="xrmdir" value="xrm.dev"/>

  <!-- Affiche en admin les extensions du mode debug -->
  <add key="displaydebugextension" value="1"/>

  <add key="reverseproxyinternalurl" value=""/>
  
  <!-- Https uniquement -->
  <add key="httpsonly" value="0"/>

  <!-- URL API -->
  <add key="apiurl" value="https:/ww2.eudonet.com"/>

  <!-- URL Externe -->
  <add key="apliexternalURL" value="https:/ww2.eudonet.com/xrm"/>


  <!-- Webbservice Volumétrie-->
  <add key="EudoVolumetrieURL" value="https://intra.eudonet.com/EudoAuditService"/>

  <!--  Authentification externe -->
  <add key="EXTERNALAUTH" value="0"/>
  <add key="EXTERNALAUTHTYPE" value="2"/>
  <add key="EXTERNALAUTHLOGIN" value="email"/>
  <add key="EXTERNALAUTHDEFAULTBASEID" value="87006F17"/>
  <add key="EXTERNALAUTHLOGVERBOSE" value="3"/>

  <!-- ADFS - NECESSITE https only -->
  <add key="ADFSApplication" value="0"/>
  <add key="ADFSDBID" value="87006F17"/>

  <!-- 0: pas de log / 1 : erreur / 2 : waring / 3 : tout -->
